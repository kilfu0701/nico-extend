{
  "manifest_version": 2,
  "name": "Nico Extend",
  "version": "0.1",
  "description": "",
  "minimum_chrome_version": "36",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    "contextMenus",
    "fileBrowserHandler",
    "fileSystem",
    "notifications",
    "storage",
    "http://*.nicovideo.jp/*"
  ]
}
